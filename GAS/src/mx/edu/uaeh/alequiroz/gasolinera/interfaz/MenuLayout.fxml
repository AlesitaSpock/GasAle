<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mx.edu.uaeh.alequiroz.gasolinera.controlador.MenuLayoutControlador">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="Menú">
            <items>
              <MenuItem fx:id="cerrarSesion" mnemonicParsing="false" text="Cerrar sesión" />
                  <MenuItem fx:id="salir" mnemonicParsing="false" text="Salir" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Ventas">
            <items>
              <MenuItem fx:id="nuevaVenta" mnemonicParsing="false" text="Nueva" />
                  <MenuItem fx:id="consultarVenta" mnemonicParsing="false" text="Consultar" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
